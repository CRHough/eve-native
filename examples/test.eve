    search
        foo = [#foo value]
        gather/top[for:(value), limit:2]
    bind
        [#max foo]
    end

    commit
        [#foo value: 1]
        [#foo value: 2]
        [#foo value: 3]
        [#foo value: 4]
        [#foo value: 5]
    end

    search
      foo = [#foo value:5]
    commit
      foo := none
    end

    search
        [#max foo: [value]]
    bind
        [#ui/text text: "max: {{value}}"]
    end
