
## Id Example 1

disabled
commit
  [#true-condition-one value: 3]
  [#true-condition-two value: 4]
end

search
  [#true-condition-one value]
bind
  [#yep value]
end

search
  [#true-condition-two value]
bind
  [#yep value]
end

search
  yeps = [#yep]
  number = gather/count[for: yeps]
bind
  [#html/div text: "I found {{number}} yeps: {{yeps}}"]
end

disabled
commit
  [#ui/button #set-value text: "Set value!"]
end

search
  [#html/event/click element: [#set-value]]
  one = [#true-condition-one]
  two = [#true-condition-two]
commit
  one.value := 5
  two.value := 5
end


## Id Example 2

commit
  thing1 = [#thing value: 1]
  thing2 = [#thing value: 2]
  thing3 = [#thing value: 3]
  [#cat minion: thing1]
end

search
  [#disable]
  cats = [#cat minion]
  number = gather/count[for: cats]
bind
  [#html/div children:
    [#html/div text: "I found {{number}} cats:" ]
    [#ui/column cats | children: 
        [#ui/text  sort: 1 text: "    Cat Id: {{cats}}"]
        [#ui/text sort: 2  text: "        {{minion}}"]
    ] 
  ]
end

## Id Example 3

commit
  [#system/timer resolution: 1000]
  [#timer #up value: 0]
  [#timer #down value: 10]
end

search
  t = [#system/timer/change tick]
  up = [#timer #up]
  down = [#timer #down]
commit
  up.value := up.value + 1
  down.value := down.value - 1
end

search
  [#timer value]
bind
  [#html/div text: value]
end